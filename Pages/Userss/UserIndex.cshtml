@page
@model SupermarketWEB.Pages.Userss.UserIndexModel
@{
    ViewData["Title"] = "UsersIndex";
}
 <h1>UserIndex user</h1>
<p>
    <a asp-page="UserCreate" class="btn btn-success">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Users[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Users[0].Password)
            </th>
          </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Users)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Password)
                </td>
                
                <td>
                    <a asp-page="./UserDelete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
                    <a asp-page="/Account/Login" class="btn btn-primary">Back To List</a>

                </td>

            </tr>
        }
    </tbody>
</table>